{
    "version": 4,
    "beforeBuildTasks": [],
    "afterBuildTasks": [],
    "global": {
        "output-debug-info": "enable",
        "use-microLIB": true
    },
    "c/cpp-compiler": {
        "optimization": "level-0",
        "one-elf-section-per-function": true,
        "C_FLAGS": "--diag_suppress=1 --diag_suppress=1295 --no-multibyte-chars --cpp",
        "CXX_FLAGS": "--diag_suppress=1 --diag_suppress=1295 --cpp",
        "warnings": "all-warnings",
        "gnu-extensions": true,
        "c99-mode": true
    },
    "asm-compiler": {},
    "linker": {
        "output-format": "elf"
    }
}